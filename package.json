{
  "name": "minnano",
  "version": "1.0.0",
  "description": "Social network for developers",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc --p tsconfig.json",
    "start": "cross-env NODE_PATH=dist node .",
    "client": "cross-env yarn --cwd \"client\" start",
    "dev:client": "cross-env concurrently -p time -t 'HH:mm' \"yarn build & yarn start\" \"yarn run client\"",
    "dev:server": "cross-env concurrently -p time -t 'HH:mm' \"tsc -w\" \"nodemon --watch ./dist --exec 'yarn start'\"",
    "dev:all": "cross-env concurrently \"yarn dev:server\" \"yarn run client\"",
    "tspath": "tspath",
    "deploy": "gulp"
  },
  "author": "Endratno Alvin",
  "license": "MIT",
  "dependencies": {
    "@google-cloud/storage": "^5.1.2",
    "@types/bcryptjs": "^2.4.2",
    "@types/config": "^0.0.37",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mongoose": "^5.10.3",
    "@types/multer": "^1.4.5",
    "@types/passport": "^1.0.4",
    "@types/passport-jwt": "^3.0.3",
    "@types/uuid": "^8.3.0",
    "@types/validator": "^13.1.1",
    "aws-sdk": "^2.787.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "config": "^3.3.3",
    "cross-env": "^7.0.3",
    "del": "^6.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.16.3",
    "fancy-log": "^1.3.3",
    "gravatar": "^1.6.0",
    "gulp": "^4.0.2",
    "gulp-zip": "^5.0.2",
    "jsonwebtoken": "^8.2.0",
    "mongoose": "^5.0.12",
    "multer": "^1.4.2",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "tslint": "^6.1.3",
    "tspath": "^1.3.7",
    "typescript": "^4.2.3",
    "uuid": "^8.3.1",
    "validator": "^9.4.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.9",
    "@types/node": "^14.14.14",
    "concurrently": "^5.2.0",
    "nodemon": "^1.17.3",
    "ts-node": "^9.1.1"
  }
}
