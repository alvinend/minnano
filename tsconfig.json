{
  "compilerOptions": {
    "baseUrl": "./",
    "target": "es6",
    "module": "commonjs",
    "rootDir": "./",
    "outDir": "./dist",
    "esModuleInterop": true,
    "strict": true,
    "importHelpers": true,
    "noImplicitAny": true,
    "moduleResolution": "node",
    "sourceMap": true
  },
  "include": [
    "./",
    "./*"
  ],
  "exclude": [
    "./client",
    "./prod-build"
  ]
}